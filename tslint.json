// 以下为个人设置的一些排除tslint提示的选项，大家可以根据自己的需要做更改
{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-shadowed-variable":false,
        "prefer-const":false,
        "no-console":false,
        "prefer-for-of":false,
        "no-eval":false,
        "max-classes-per-file":false
    },
    "rulesDirectory": []
}